  "keywords": [
    "cli",
    "manifest",
    "protocol",
    "data"
  ],
  "author": "Cross-Protocol Manifest System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kneelinghorse/Cross-Protocol-Manifest-System",
    "directory": "packages/cli"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
