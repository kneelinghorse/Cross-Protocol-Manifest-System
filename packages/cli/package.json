{
  "name": "@cpms/cli",
  "version": "0.4.1",
  "private": false,
  "description": "Proto CLI for validating, diffing, and generating manifests",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "proto": "./dist/proto.js"
  },
  "main": "./dist/proto.js",
  "module": "./dist/proto.js",
  "exports": {
    ".": "./dist/proto.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
