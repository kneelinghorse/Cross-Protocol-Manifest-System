{
  "name": "cross-protocol-manifest-system",
  "version": "1.0.0",
  "private": true,
  "description": "Zero-dependency cross-protocol manifest system",
  "main": "utils.js",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "pnpm build && node --test agent-protocol.test.js api-protocol.test.js data-protocol.test.js event-protocol.test.js proto.test.js utils.test.js urn-resolver.test.js",
    "test:workspace": "turbo run test",
    "test:coverage": "node --test --experimental-test-coverage",
    "benchmark": "node benchmark.js",
    "lint": "turbo run lint",
    "check:size": "turbo run check-size",
    "changeset": "changeset",
    "version-packages": "pnpm changeset version",
    "release": "pnpm changeset publish"
  },
  "keywords": [
    "manifest",
    "protocol",
    "zero-dependency",
    "data-protocol"
  ],
  "author": "Cross-Protocol Manifest System Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.12.0",
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@changesets/changelog-github": "^0.5.0",
    "turbo": "^2.1.2"
  }
}
