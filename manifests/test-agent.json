{
  "urn": "urn:proto:agent:support@v1.0.0",
  "type": "agent",
  "name": "Customer Support Agent",
  "version": "1.0.0",
  "dataset": {
    "name": "support_agent"
  },
  "capabilities": {
    "tools": ["refund", "escalate", "lookup_order"]
  },
  "schema": {
    "fields": [
      {
        "name": "ticket_id",
        "type": "string",
        "description": "Stable identifier for the support ticket"
      },
      {
        "name": "intent",
        "type": "string",
        "description": "Predicted reason for contacting support"
      },
      {
        "name": "priority",
        "type": "string",
        "description": "Current ticket priority label"
      }
    ]
  },
  "dependencies": {
    "requires": [
      "urn:proto:api:billing@v2.0.0",
      "urn:proto:data:user_events@v1.1.1"
    ]
  }
}
